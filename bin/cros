#!/bin/bash
#
sudo curl -fsSL https://download.docker.com/linux/debian/gpg | sudo apt-key add -
sudo echo "deb [arch=amd64] https://download.docker.com/linux/debian $(lsb_release -cs) stable" > /etc/apt/sources.list.d/docker.list
sudo apt update
sudo apt upgrade -y
sudo apt install -y docker-ce laspass-cli expect jq
#
sudo usermod -aG docker alimustakim
#